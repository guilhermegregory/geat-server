{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "no-undef": "off",
        "arrow-body-style": "off",
        "object-curly-newline": [
            "error",
            {
                "multiline": true
            }
        ],
        "max-len": "off",
        "no-console": "off"
    }
}